/* wait.qss */

QPushButton#waitbutton {
    background-color: rgb(0, 255, 0);
    font-size: 100pt;
    font-weight:900;
    border-radius: 20px;
    color: black;
}

QPushButton#waitbackbutton {
    background-color: rgb(250,250,250);
    font: 10pt "Sans Serif";
    border-radius: 10px;
    color: white;
}

QPushButton#waitbackbutton:pressed {
    background-color: rgb(245,245,245);
    color: white;
}

